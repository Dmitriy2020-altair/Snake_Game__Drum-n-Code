{
	"extends": [
		"react-app",
		"react-app/jest",
		"airbnb",
		"airbnb/hooks"
	],
	"rules": {
		"no-console": "warn",
		"react/button-has-type": "off",
		"react/no-array-index-key": "off",
		"no-plusplus": "off",
		"react/prop-types": "off",
		"prefer-const": "off",
		"consistent-return": "off",
		"no-restricted-syntax": "off",
		"max-len": "off",
		"no-use-before-define": "off",
		"no-alert": "off"
	}
}